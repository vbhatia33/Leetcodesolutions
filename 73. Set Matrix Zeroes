// space 0(m+n) solution  
    void setrow(vector<vector<int>>&matrix,int row,int m,int n){
            for(int i=0;i<n;i++){
                matrix[row][i]=0;
            }
        }
        void setcol(vector<vector<int>>&matrix,int col,int m,int n){
            for(int i=0;i<m;i++){
                matrix[i][col]=0;
            }
        }
    void setZeroes(vector<vector<int>>&matrix) {
        int m=matrix.size();
        int n=matrix[0].size();
        vector<int>rowz;
        vector<int>colz;
        for(int i=0;i<m;i++){
            for(int j=0;j<n;j++){
                if(matrix[i][j]==0){
                    rowz.push_back(i);
                    colz.push_back(j);
                }
            }
        }
        for(auto i:rowz){
            setrow(matrix,i,m,n);
        }
        for(auto i:colz){
            setcol(matrix,i,m,n);
        }
        return;
    }
